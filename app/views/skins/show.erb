<h1><%= @skins.name %></h1>
<%= image_tag @skins.photo_url, class: 'img-responsive' %>
<div class="well">
  <p><%= @skins.description %></p>
</div>

<% if user_signed_in? %>
<div>

  <li><%= link_to "Edit", edit_skin_path(@skins) %></li>
  <%= button_to "Destroy", @skins , 
                        method: :delete, 
                        data: { confirm: 'Are you sure?' } %>
</div>
<% end %>
<br><br><br><br>